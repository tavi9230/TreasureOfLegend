{
  "parser": "babel-eslint",
  "env": {
    "browser": true,
    "node": true
  },
  "rules": {
    "quotes": [ 2, "single" ],
    "semi": 2,
    "eqeqeq": 2,
    "no-duplicate-case": 2,
    "no-empty": 2,
    "no-extra-boolean-cast": 2,
    "no-extra-semi": 2,
    "no-irregular-whitespace": 2,
    "no-unreachable": 2,
    "use-isnan": 2,
    "default-case": 2,
    "no-multi-spaces": 2,
    "no-useless-return": 2,
    "no-unused-vars": 2,
    "no-use-before-define": 2,
    "array-bracket-newline": 2,
    "array-bracket-spacing": 2,
    "camelcase": 2,
    "comma-dangle": 2,
    "comma-spacing": 2,
    "eol-last": [ "error", "never" ],
    "key-spacing": [
      "error",
      { "afterColon": true }
    ],
    "keyword-spacing": [
      "error",
      { "before": true }
    ],
    "max-len": [
      "error",
      {
        "code": 200,
        "tabWidth": 4,
        "comments": 200,
        "ignoreUrls": true,
        "ignoreRegExpLiterals": true
      }
    ],
    "no-multiple-empty-lines": 2,
    "no-trailing-spaces": 2,
    "no-whitespace-before-property": 2 
  }
}